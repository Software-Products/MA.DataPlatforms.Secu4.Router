    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
      <Title>MA.DataPlatform.Secu4.KafkaMetadataComponent</Title>
      <Company>McLaren Applied</Company>
      <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
      <Authors>McLaren Applied</Authors>
      <Description>Library containing utilities to fetch metadata of a Kafka broker.</Description>
